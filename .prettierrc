{
  "endOfLine": "lf",
  "bracketSpacing": true,
  "useTabs": false,
  "quoteProps": "as-needed",
  "proseWrap": "preserve",
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "tabWidth": 2,
  "arrowParens": "avoid",
  "printWidth": 120,
  "htmlWhitespaceSensitivity": "css",
  "bracketSameLine": true,
  "jsxSingleQuote": false,
  "embeddedLanguageFormatting": "auto",
  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": false,
  "overrides": [
    {
      "files": "*.{ts,tsx}",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": "*.{js,jsx}",
      "options": {
        "parser": "babel"
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    }
  ]
}
